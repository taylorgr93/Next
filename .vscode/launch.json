{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\02-my-dashboard\\src\\app\\dashboard\\pokemon\\[id]\\page.tsx",
      "outFiles": ["${workspaceFolder}/**/*.js"]
    },
    {
      "type": "node-terminal",
      "name": "Run Script: start",
      "request": "launch",
      "command": "npm run start",
      "cwd": "${workspaceFolder}\\01-first-steps"
    }
  ]
}
